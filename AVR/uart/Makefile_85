PRG            = attiny85
OBJ            = $(PRG).o
ATMEGA_NUM     = 85
MCU_TARGET     = attiny${ATMEGA_NUM}

OPTIMIZE       = -Os

DEFS           =
LIBS           =

include ../make.generic.C++

burn: $(PRG).hex
	avrdude -P$(TTY) -pt${ATMEGA_NUM} -v -cstk500v2 -Uflash:w:./$(PRG).hex

erase:
	avrdude -P$(TTY) -e -pt${ATMEGA_NUM} -v -cstk500v2
