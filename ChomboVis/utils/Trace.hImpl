#ifndef INCLUDED_TRACE_HIMPL
#define INCLUDED_TRACE_HIMPL

#include "cmdline_base.h"

template< typename T> TraceStream &
operator<<( TraceStream & tstr, T const & t )
{
    if( CmdlineBase::DebugLevel() >= tstr._debug_level )
    {
        tstr._ost << t;
    }
    return tstr;
}


#endif // INCLUDED_TRACE_HIMPL
